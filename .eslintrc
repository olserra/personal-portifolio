{
  "extends": "next",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
},
  "rules": {
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off",
    // for use in edge api routes
    "@next/next/no-server-import-in-page": "off"
  }
}
